<h1>Add A Collaborator</h1>

<%= form_for [wiki, Collaboration.new] do |f| %>
  <div class="form-group">
    <%= f.collection_select :user_id, User.not_collaborator_yet(wiki), :id, :email %>
  </div>
  <div class="form-group">
    <%= f.submit "Add Collaborator", class: 'btn btn-success' %>
  </div>
<% end %>
